{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "wip",
  "title": "WIP",
  "description": "List your WIP todos.",
  "icon": "icon.svg",
  "author": "marckohlbrugge",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show your WIP todos",
      "description": "Template for showing a static list and a detail view",
      "mode": "view"
    },
    {
      "name": "streak",
      "title": "WIP",
      "subtitle": "Show WIP Streak",
      "description": "Show WIP Streak",
      "mode": "no-view",
      "interval": "10s"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.66.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}